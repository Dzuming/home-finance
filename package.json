{
  "name": "home-finance",
  "version": "1.0.0",
  "description": "Simple application to monitoring home finance",
  "main": "src/index.js",
  "scripts": {
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "lint": "esw webpack.config.* src tools --color && npm run sass-lint",
    "lint:watch": "npm run lint -- --watch",
    "sass-lint": "sass-lint -c .sass-lint.yml -v -q",
    "prebuild": "run-p clean-dist lint test",
    "build": "babel-node tools/build.js",
    "test": "echo \"Error: no test specified\"",
    "serve": "webpack-dev-server --hot --inline",
    "start": "run-p serve lint:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dzuming/home-finance.git"
  },
  "keywords": [
    "react",
    "react.js",
    "finance"
  ],
  "author": "Dawid Poliszak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dzuming/home-finance/issues"
  },
  "homepage": "https://github.com/Dzuming/home-finance#readme",
  "dependencies": {
    "install": "^0.10.1",
    "material-ui": "^1.0.0-beta.11",
    "material-ui-icons": "^1.0.0-beta.10",
    "npm": "^5.4.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "7",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.1.0",
    "css-loader": "^0.28.7",
    "eslint": "3.x",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "html-webpack-plugin": "^2.30.1",
    "npm-run-all": "^4.1.1",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-md5-hash": "^0.0.5"
  }
}
