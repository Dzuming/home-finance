{
  "name": "home-finance",
  "version": "1.0.0",
  "description": "Simple application to monitoring home finance",
  "main": "src/index.js",
  "scripts": {
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "lint": "esw webpack.config.* src tools --color && npm run sass-lint",
    "lint:watch": "npm run lint -- --watch",
    "sass-lint": "sass-lint -c .sass-lint.yml -v -q",
    "prebuild": "run-p clean-dist lint test",
    "build": "babel-node tools/build.js",
    "test": "jest",
    "update-snapshot": "jest --updateSnapshot",
    "serve": "webpack-dev-server --hot --inline",
    "start": "run-p serve lint:watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dzuming/home-finance.git"
  },
  "keywords": [
    "react",
    "react.js",
    "finance"
  ],
  "author": "Dawid Poliszak",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dzuming/home-finance/issues"
  },
  "homepage": "https://github.com/Dzuming/home-finance#readme",
  "dependencies": {
    "@devexpress/dx-react-core": "^1.0.0-rc.2",
    "@devexpress/dx-react-grid": "^1.0.0-rc.2",
    "@devexpress/dx-react-grid-material-ui": "^1.0.0-rc.2",
    "install": "^0.10.1",
    "material-ui": "^1.0.0-beta.11",
    "material-ui-icons": "^1.0.0-beta.10",
    "normalize.css": "^7.0.0",
    "npm": "^5.4.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux-thunk": "^2.2.0",
    "thunk": "^0.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "8",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.1.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "4.x",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-watch": "^3.1.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "fetch-mock": "^6.0.0-beta.9",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.1.4",
    "jest-cli": "^22.1.4",
    "jest-localstorage-mock": "^2.2.0",
    "npm-run-all": "^4.1.1",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "react-addons-test-utils": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-test-renderer": "^16.1.1",
    "redux": "^3.7.2",
